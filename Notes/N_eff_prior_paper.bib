@article{okamoto:2003,
	Abstract = {Gravitational lensing of the microwave background by the intervening dark matter mainly arises from large-angle fluctuations in the projected gravitational potential and hence offers a unique opportunity to study the physics of the dark sector at large scales. Studies with surveys that cover greater than a percent of the sky will require techniques that incorporate the curvature of the sky. We lay the groundwork for these studies by deriving the full sky minimum variance quadratic estimators of the lensing potential from the CMB temperature and polarization fields. We also present a general technique for constructing these estimators, with harmonic space convolutions replaced by real space products, that is appropriate for both the full sky limit and the flat sky approximation. This also extends previous treatments to include estimators involving the temperature-polarization cross-correlation and should be useful for next generation experiments in which most of the additional information from polarization comes from this channel due to sensitivity limitations.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2003PhRvD..67h3002O},
	Arxivurl = {http://arXiv.org/abs/astro-ph/0301031},
	Author = {{Okamoto}, T. and {Hu}, W.},
	Date-Added = {2015-08-04 22:16:40 +0000},
	Date-Modified = {2015-08-04 22:16:40 +0000},
	Doi = {10.1103/PhysRevD.67.083002},
	Eid = {083002},
	Eprint = {astro-ph/0301031},
	Journal = {\prd},
	Keywords = {Background radiations},
	Month = apr,
	Number = 8,
	Pages = {083002},
	Title = {{Cosmic microwave background lensing reconstruction on the full sky}},
	Volume = 67,
	Year = 2003,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1103/PhysRevD.67.083002},
	Bdsk-Url-2 = {http://arXiv.org/abs/astro-ph/0301031},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2003PhRvD..67h3002O}}

@article{hu:2002,
	Abstract = {Weak gravitational lensing by the intervening large-scale structure
	of the universe induces high-order correlations in the cosmic microwave
	background temperature and polarization fields. We construct minimum
	variance estimators of the intervening mass distribution out of the
	six quadratic combinations of the temperature and polarization fields.
	Polarization begins to assist in the reconstruction when E-mode mapping
	becomes possible on degree-scale fields, i.e., for an experiment
	with a noise level of ~40 muK arcmin and beam of ~7', similar to
	the Planck experiment; surpasses the temperature reconstruction at
	~26 muK arcmin and 4' and yet continues to improve the reconstruction
	until the lensing B-modes are mapped to l~2000 at ~0.3 muK arcmin
	and 3'. Ultimately, the correlation between the E- and B-modes can
	provide a high signal-to-noise ratio mass map out to multipoles of
	L~1000, extending the range of temperature-based estimators by nearly
	an order of magnitude. We outline four applications of mass reconstruction:
	measurement of the linear power spectrum in projection to the cosmic
	variance limit out to L~1000 (or wavenumbers 0.002<~k<~0.2 in h Mpc-1),
	cross-correlation with cosmic shear surveys to probe the evolution
	of structure tomographically, cross-correlation of the mass and temperature
	maps to probe the dark energy, and the separation of lensing and
	gravitational wave B-modes.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2002ApJ...574..566H},
	Arxivurl = {http://arXiv.org/abs/astro-ph/0111606},
	Author = {{Hu}, W. and {Okamoto}, T.},
	Date-Added = {2015-08-04 22:16:40 +0000},
	Date-Modified = {2015-08-04 22:16:40 +0000},
	Doi = {10.1086/341110},
	Eprint = {arXiv:astro-ph/0111606},
	Journal = {\apj},
	Keywords = {Cosmology: Cosmic Microwave Background, Cosmology: Dark Matter, Cosmology: Large-Scale Structure of Universe},
	Month = aug,
	Pages = {566-574},
	Title = {{Mass Reconstruction with Cosmic Microwave Background Polarization}},
	Volume = {574},
	Year = {2002},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1086/341110},
	Bdsk-Url-2 = {http://arXiv.org/abs/astro-ph/0111606},
	Bdsk-Url-3 = {http://adsabs.harvard.edu/abs/2002ApJ...574..566H}}

@article{pan:2015,
	Abstract = {Our tightest upper limit on the sum of neutrino mass eigenvalues $M_\nu$ comes from cosmological observations that will improve substantially in the near future, enabling a detection. The combination of the Baryon Acoustic Oscillation feature measured from the Dark Energy Spectroscopic Instrument and a Stage-IV Cosmic Microwave Background experiment has been forecasted to achieve $\sigma(M_\nu) < 1/3$ of the lower limit on $M_\nu$ from atmospheric and solar neutrino oscillations \citep{2013arXiv1309.5383A,2012PhRvD..86a3012F}. Here we examine in detail the physical effects of neutrino mass on cosmological observables that make these constraints possible. We also consider how these constraints would be improved to ensure at least a $5\sigma$ detection.},
	Adscomment = {7 pages, 7 figs},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2015arXiv150607493P},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1506.07493},
	Author = {{Pan}, Z. and {Knox}, L.},
	Date-Added = {2015-08-04 21:21:24 +0000},
	Date-Modified = {2015-08-04 21:21:24 +0000},
	Eprint = {1506.07493},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = jun,
	Title = {{Constraints on neutrino mass from Cosmic Microwave Background and Large Scale Structure}},
	Year = 2015,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://arXiv.org/abs/1506.07493},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/abs/2015arXiv150607493P}}

